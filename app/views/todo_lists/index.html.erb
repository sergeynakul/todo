<div class="row">
  <div class="col">
    <h1>Your TODO lists</h1>

    <div class="create-errors">
      <%= render 'shared/errors', resource: @todo_list %>
    </div>

    <%= render 'todo_lists/form_inline', todo_list: @todo_list %>

    <table class="table">
      <tr>
        <th>Title</th>
        <th colspan="3">Actions</th>
      </tr>
      <%= render @todo_lists %>
    </table>
  </div>
</div>
